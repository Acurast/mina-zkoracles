{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:contracts": "cd ../contracts && npm install && npm run build && cd ../frontend",
    "build": "npm run build:contracts && next build",
    "start": "next start",
    "lint": "next lint",
    "ts-watch": "tsc --noEmit --incremental --watch",
    "export": "next export",
    "deploy": "npm run build && touch out/.nojekyll && node ./ghp-postbuild && git add -f out && git commit -m \"Deploy gh-pages\" && cd .. && git subtree push -f --prefix frontend/out origin gh-pages"
  },
  "dependencies": {
    "@types/node": "20.10.1",
    "@types/react": "18.2.39",
    "@types/react-dom": "18.2.17",
    "autoprefixer": "10.4.16",
    "coi-serviceworker": "^0.1.7",
    "eslint": "8.54.0",
    "eslint-config-next": "14.0.3",
    "next": "14.0.3",
    "o1js": "0.*",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.5",
    "typescript": "5.3.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7"
  }
}
